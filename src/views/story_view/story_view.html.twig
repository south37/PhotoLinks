{% extends 'layout.html.twig' %}

{% block contents %}
    {% include 'error.html.twig' with {errors: errors} %}

    <div class="row">
        <div class="span6 offset3">
            <h1>title</h1>
            <!--画像表示するところ-->
            {% for i in 0..9 %}
                {{ imgPathList[i] }}
                <img src="{{ imgPathList[i] }}" alt="パス不明">
            {% endfor %}

            <!--selectに飛ばす-->
            <form action = "{{ urlFor('welcome') }} ">
                 <div class="controls">
                    <button id="return_select" type="submit" class="btn btn-primary">コマ選択に戻る</button>
                </div>
            </form>

        </div>
    </div>
{% endblock %}
